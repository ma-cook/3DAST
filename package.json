{
  "name": "3d-ast-generator",
  "version": "1.0.3",
  "description": "A TypeScript library for parsing Merfolk syntax and generating 3D abstract syntax trees for visualization",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "3d-ast": "dist/src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "ast",
    "3d",
    "visualization",
    "merfolk",
    "3d-ast",
    "graph",
    "parser",
    "typescript"
  ],
  "author": "3D AST Generator Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ma-cook/3DAST"
  },
  "bugs": {
    "url": "https://github.com/ma-cook/3DAST"
  },
  "homepage": "https://github.com/ma-cook/3DAST",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
